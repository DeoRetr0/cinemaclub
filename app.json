{
  "expo": {
    "name": "cinemaclub",
    "slug": "cinemaclub",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "enableDangerousExperimentalLeanBuilds": true
    },
    "ios": {
      "supportsTablet": true
    },
    "androidNavigationBar": {
     "backgroundColor": "#1C2646"
   }
  }
}
